    <property>
      <code>#template('colorThemeInit.vm')

/**********************************************************************************************
* Tabs css.
*/
#document-head .title-tab {
  display: inline-block;
  vertical-align: top;
  line-height: 0;
}

#document-head .title-tab h1 {
  text-transform: none !important;
}

#document-head .title-tab:not(.active) {
  background-color: #EEE;
  background-image: linear-gradient(to bottom, #EEE 0%, #DDD 50%);
  border-radius: 8px 8px 0 0;
  box-shadow: 0 -3px 3px rgba(0, 0, 0, 0.15);
  font-size: 1.2em;
  font-weight: bold;
  letter-spacing: 2px;
  margin: 0 3px;
  padding: 0.4em 11px;
  text-shadow: 0 0 1px #FFFFFF;
}

#document-head .title-tab a {
  color: $theme.textColor;
}

#document-head .title-tab a:hover {
  color: $theme.linkColor;
  text-decoration: none;
}</code>
